{
  "manifest_version": 3,
  "name": "ChromeKiwi",
  "version": "1.0",
  "description": "A cute Kiwi Bird that walks around your screen!",
  "permissions": [],
  "icons": {
        "16": "assets/icon16.png",
        "32": "assets/icon32.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
     },
      
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
